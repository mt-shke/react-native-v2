{
    "expo": {
        "name": "08_sneakers_style",
        "slug": "08_sneakers_style",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/img/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./src/assets/img/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
